	<parameter name="NOME_CLIENTE" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT corretora.aluguel.`dataPagamento` AS PAGAMENTO,
	corretora.aluguel.`dataVencimento` AS VENCIMENTO,
	corretora.aluguel.obs AS OBS,
	corretora.aluguel.`valorPago` AS 'PAGO',
	corretora.cliente.`nomeCliente` AS CLIENTE,
	corretora.cliente.email AS EMAIL,
	corretora.cliente.`CPF` AS CPF,
	corretora.locacao.`valorAluguel` AS 'VALOR ALUGUEL',
	corretora.locacao.`percentualMulta` AS 'PORCENTAGEM MULTA'
FROM corretora.aluguel
	INNER JOIN corretora.locacao ON 
	 corretora.aluguel.locacao_id = corretora.locacao.id 
	INNER JOIN corretora.cliente ON 
	 corretora.locacao.inquilino_id = corretora.cliente.id
	 WHERE corretora.cliente.nomeCliente =  $P{NOME_CLIENTE}]]>
	</queryString>
