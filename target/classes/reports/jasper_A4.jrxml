		<queryString language="SQL">
			<![CDATA[SELECT corretora.imovel.bairro AS BAIRRO,locacao.valorAluguel AS VALOR ,count(*) as N_LOCACAO FROM corretora.locacao
inner join corretora.imovel
on corretora.imovel.id = corretora.locacao.imovel_id
group by corretora.locacao.imovel_id
order by N_LOCACAO desc
limit 3]]>
		</queryString>
